name: "dbm"
model_type: DBM
layer {
  name: "input_layer"
  dimensions: 100
  numlabels: 20
  param {
    name: "bias"
    initialization: PRETRAINED
    pretrained_model: "rbm1_BEST"
  }
  is_input: true
  loss_function: CROSS_ENTROPY
  hyperparams {
    sparsity: false
    dropout: false
    activation: SOFTMAX
  }
  data_field {
    train: "train_data"
    validation: "validation_data"
    test: "test_data"
  }
  performance_stats {
    compute_correct_preds: true
    compute_cross_entropy: true
    compute_error: false
  }
}
layer {
  name: "hidden1"
  dimensions: 1000
  param {
    name: "bias"
    initialization: PRETRAINED
    pretrained_model: "rbm1_BEST"
  }
  hyperparams {
    activation: LOGISTIC
  }
  performance_stats {
    compute_sparsity: true
  }
}
layer {
  name: "hidden2"
  dimensions: 1000
  param {
    name: "bias"
    initialization: PRETRAINED
    pretrained_model: "rbm2_BEST"
  }
  hyperparams {
    activation: LOGISTIC
  }
  performance_stats {
    compute_sparsity: true
  }
}
edge {
  node1: "input_layer"
  node2: "hidden1"
  directed: false
  param {
    name: "weight"
    initialization: PRETRAINED
    pretrained_model: "rbm1_BEST"
  }
  up_factor: 0.5
}
edge {
  node1: "hidden1"
  node2: "hidden2"
  directed: false
  param {
    name: "weight"
    initialization: PRETRAINED
    pretrained_model: "rbm2_BEST"
  }
  down_factor: 0.5
}
hyperparams {
  base_epsilon: 0.1
  epsilon_decay: INVERSE_T
  epsilon_decay_half_life: 20000
  initial_momentum: 0.1
  final_momentum: 0.001
  momentum_change_steps: 3000
  sparsity: true
  sparsity_target: 0.5
  sparsity_cost: 0.01
  sparsity_damping: 0.9
  apply_l2_decay: true
  l2_decay: 0.001
  activation: LOGISTIC
  mf_steps: 3
  gibbs_steps: 3
  enable_display: false
  select_model_using_cross_entropy: true
}
prefix: "/storage/data1/dbm/deepnet/deepnet/examples/results/exp8/dbm_models"
